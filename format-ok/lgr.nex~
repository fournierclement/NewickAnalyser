#nexus
[ un fichier avec des arbres ayant des longueurs de branche ]

BEGIN taxa;
DIMENSIONS ntax=4;
TAXLABELS
[1] t1
[2] t2
[3] t3
[4] t4
;
END [taxa];

BEGIN trees;
[ci dessous "t:0.20125" signifie que l'arête menant à t1 dans l'arbre]
[ a une longueur de 0.20125]
tree ab= [&R] (t1:0.20125,(t2:0.1875,(t3:0.0425,t4:0.1575):0.0125):0.20125):0;
tree NJ= [&R] (t2:0.20125,(t4:0.1875,(t1:0.0425,t3:0.1575):0.0125):0.20125):0;
tree NJ= [&R] (t1:0.03,(t2:0.37,(t4:0.125,t3:0.095):0.02):0.03):0;
tree NJ= [&R] (t1:0.02125,(t3:0.1075,(t4:0.1625,t2:0.3475):0.0125):0.02125):0;
tree NJ= [&R] (t1:0.03375,(t2:0.2025,(t3:0.1575,t4:0.2225):0.0175):0.03375):0;
END [trees];
